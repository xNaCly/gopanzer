# xnacly's panzer config
# https://github.com/xnacly

# --- env ---
set PROMPT "\9\d\0 \1\b\0 \3\S\0 \8$\0 "
set EDITOR "nvim"

# --- alias ---
alias .. "cd .."

# vim
alias vim "nvim"

alias ls "exa"
alias ll "exa -l"
alias la "exa -la"
alias tree "exa --tree --level=4 --icons --git"

# git
alias gs "git status"
alias ga "git add -A"
alias gpl "git pull"
alias gps "git push"
alias gc "git commit -m "

# wsl
alias c "cmd.exe /c "
